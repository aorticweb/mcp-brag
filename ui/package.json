  "description": "MCP RAG App",
  "main": ".vite/build/main.js",
  "engines": {
    "node": "^22.9.0"
  },
  "scripts": {
    "dev": "electron-vite dev",
    "build": "npm run typecheck && npm run lint && electron-vite build",
    "build:main": "electron-vite build --config electron.vite.config.ts",
    "build:preload": "electron-vite build --config electron.vite.config.ts",
    "build:renderer": "electron-vite build --config electron.vite.config.ts",
    "preview": "electron-vite preview",
    "start": "electron-forge start",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,jsx,css,json,md}'",
    "clean": "rimraf dist out .vite",
    "clean:all": "npm run clean && rimraf node_modules"
  },
